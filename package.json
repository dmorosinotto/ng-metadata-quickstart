{
  "name": "ng-metadata-quickstart",
  "version": "1.0.0",
  "description": "Sample project quickstart using ngMetadata",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --config tslint.json app/*.ts test/*.ts app/**/*.ts test/**/*.ts",
    "prebuild": "npm run lint",
    "build": "tsc  -pretty  -p .",
    "preserve": "npm run build",
    "serve": "live-server",
    "start": "npm run build  &  npm run serve",
    "test": "tsc -p . && karma start",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmorosinotto/ng-metadata-quickstart.git"
  },
  "keywords": [
    "ngMetadata",
    "Angular",
    "Typescript",
    "SystemJS",
    "karma"
  ],
  "author": "Daniele Morosinotto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmorosinotto/ng-metadata-quickstart/issues"
  },
  "homepage": "https://github.com/dmorosinotto/ng-metadata-quickstart#readme",
  "dependencies": {
    "angular": "^1.5.3",
    "ng-metadata": "^1.5.2",
    "reflect-metadata": "^0.1.3",
    "systemjs": "^0.19.26",
    "systemjs-plugin-css": "^0.1.20",
    "systemjs-plugin-text": "git+https://github.com/systemjs/plugin-text.git",
    "traceur": "0.0.107"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.3",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "inherits": "^2.0.1",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "live-server": "^0.9.2",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
    "remap-istanbul": "^0.6.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "systemjs-builder": "^0.15.15",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "typings": "^0.7.12",
    "util": "^0.10.3"
  }
}
